---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>{{ site.name }}'s Blog</title>
	<link href="http://{{site.github.owner_name}}.github.io{{ page.url }}" rel="self"/>
	<link href="http://{{site.github.owner_name}}.github.io/blog/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://{{site.github.owner_name}}.github.io/blog/</id>
	<author>
		<name>{{ site.name }}</name>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://{{site.github.owner_name}}.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://{{site.github.owner_name}}.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
