---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Muhammad Naqvi's Notes</title>
	<link href="http://monaqvi.github.io/{{ page.url }}" rel="self"/>
	<link href="http://monaqvi.github.io/notes/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://monaqvi.github.io/notes/</id>
	<author>
		<name>Muhammad Naqvi</name>
		<email>naqvi.muhammad@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://monaqvi.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://monaqvi.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
